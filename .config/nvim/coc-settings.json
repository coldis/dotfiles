{
	"coc.preferences.formatOnSaveFiletypes": ["css", "javascript", "html", "json", "yaml", "terraform", "hcl"],
	"eslint.autoFixOnSave": true,
	"list.normalMappings": {
	  "v": "action:vsplit",
	  "s": "action:split"
	},
	"list.insertMappings": {
	  "<C-v>": "action:vsplit",
	  "<C-s>": "action:split"
	},
	"list.source.files.args": ["--files", "--hidden"],
  "tsserver.reportStyleChecksAsWarnings": false
}
